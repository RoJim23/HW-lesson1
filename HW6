my_dict = {'Roma': 1994, 'Polina': 2000, 'Sasha': 1999}
print(my_dict)
print(my_dict['Roma'])
print(my_dict.get('Masha' , 'без ошибки'))
my_dict.update({'Pasha' : 2001, 'Lexa': 2002})
print(my_dict)
a = my_dict.pop('Polina')
print(a)
print(my_dict)

my_set= {24, 25, 27, 23, 25, 27, 27, 'dog'}
print(my_set)
my_set.add (21)
my_set.add(15)
my_set.discard(23)
print(my_set)
